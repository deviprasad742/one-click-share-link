{
    "manifest_version": 2,

    "name": "1-Click Share Link Public",
    "description": "Share and receive links with 1 click",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "background": {
        "page": "background.html"
    },

    "permissions": [
    "tabs",
    "activeTab",
    "https://one-click-share-link-public.herokuapp.com/links",
    "*://*/*/"
  ]
}