{
    "manifest_version": 2,

    "name": "1-Click Share Link",
    "description": "Share and receive links with 1 click",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "page": "background.html"
    },

    "permissions": [
    "tabs",
    "activeTab",
    "https://one-click-share-link.herokuapp.com/links",
    "*://*/*/"
  ]
}