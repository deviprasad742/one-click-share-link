{
  "manifest_version": 2,

  "name": "1-Click Link Share",
  "description": "Share and receive links with 1 click",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "http://one-click-share-link.herokuapp.com/links",
    "*://*/*/"
  ]
}
